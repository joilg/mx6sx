	status = "okay";
};

&uart5 { /* for IMX Debug-Uart */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	fsl,uart-has-rtscts;
	status = "okay";
};

/*  pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usb_otg1_id>;
	dr_mode = "peripheral";


*/

&usbotg1 {
	vbus-supply = <&reg_usb_otg1_vbus>;
	pinctrl-names = "default";
/*	pinctrl-0 = <&pinctrl_usb_otg1_id>; */
	dr_mode = "peripheral";
/*	dr_mode = "host"; */
	status = "okay";
};

&usbotg2 {
	vbus-supply = <&reg_usb_otg2_vbus>;
	dr_mode = "peripheral";
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	no-1-8-v;
	wlan-supply = <&wlreg_on>;
	keep-power-in-suspend;
	cap-power-off-card;
	enable-sdio-wakeup;
	max-frequency = <20000000>;
	bus-width = <4>;
	status = "okay";

	mcmdhd_wlan_0: mcmdhd_wlan@0 {
		compatible = "marvell,88w8887";
		wlreg_on-supply = <&wlreg_on>;
	};

	mwifiex: mwifiex@1 {
		compatible = "marvell,sd8xxx";
		reg = <1>;
		interrupt-parent = <&gpio2>;
		interrupts = <10 IRQ_TYPE_LEVEL_LOW>;
		marvell,caldata_00_txpwrlimit_2g_cfg_set = /bits/ 8 <
	0x01 0x00 0x06 0x00 0x08 0x02 0x89 0x01>;
		marvell,wakeup-pin = <1>;
	};
	
	btmrvl: btmrvl@2 {
		compatible = "marvell,sd8xxx-bt";
		reg = <2>;
		interrupt-parent = <&gpio2>;
		interrupts = <11 IRQ_TYPE_LEVEL_LOW>;
		btmrvl,cal-data = /bits/ 8 <
			0x37 0x01 0x1c 0x00 0xff 0xff 0xff 0xff 0x01 0x7f 0x04 0x02
			0x00 0x00 0xba 0xce 0xc0 0xc6 0x2d 0x00 0x00 0x00 0x00 0x00
			0x00 0x00 0xf0 0x00>;
		btmrvl,wakeup-pin = <0x0d>;
		btmrvl,wakeup-gap = <0x64>;
	};
};

