	phy-mode = "rmii";
	phy-id = <0>;
	phy-handle = <&ethphy2>;
	status = "okay";

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethphy1: ethernet-phy@1 {
			reg = <1>;
			max-speed = <100>;
		};

		ethphy2: ethernet-phy@2 {
			reg = <0>;
			max-speed = <100>;
		};
	};
};

