/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "imx6sx.dtsi"

/ {
	model = "Continental i.MX6 SoloX VWBLAN Board";
	compatible = "fsl,imx6sx-vwblan", "fsl,imx6sx";

	chosen {
		stdout-path = &uart1;
	};

	memory {
		reg = <0x80000000 0x4000000>;
	};

	/* LEDs
	Name	gpio		pin				active
	LED1	GPIO4_IO08	NAND_DATA04 	High
	LED2	GPIO4_IO10	NAND_DATA06 	High
	LED3	GPIO6_IO15	SD4_DATA1	 	High
	LED4	GPIO6_IO14	SD4_DATA0 		High
	*/
	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;

		led1: sleep {
			label = "sleep";
			gpios = <&gpio4 8 1>;
			default-state = "on";
			linux,default-trigger = "default-on";
		   };
		led2: lan {
			label = "lan";
			gpios = <&gpio4 10 1>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		   };
		led3: dsrc_tx {
			label = "dsrc_tx";
			gpios = <&gpio6 15 1>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		   };
		led4: dsrc_rx {
			label = "dsrc_rx";
			gpios = <&gpio6 14 1>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		   };
	};

	/* GPIOs
	Name	gpio		pin				active
	SW1		*** 5V ***
	SW2		GPIO1_IO10	GPIO1_IO10 		Low
	SW3		GPIO7_IO10	USB_H_DATA 		Low
	SW4		GPIO7_IO11	USB_H_STROBE 	Low
	*/
	gpio_keys_polled {
		compatible = "gpio-keys-polled";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_keys_polled>;
		poll-interval = <1000>;
		#address-cells = <1>;
		#size-cells = <0>;

		sw2 {
			label = "sw2";
			gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
			linux,code = <49>;
		};
		sw3 {
			label = "sw3";
			gpios = <&gpio7 10 GPIO_ACTIVE_LOW>;
			linux,code = <50>;
		};
		sw4 {
			label = "sw4";
			gpios = <&gpio7 11 GPIO_ACTIVE_LOW>;
			linux,code = <51>;
		};
	};

	pps {
		compatible = "pps-gpio";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_pps>;
		label = "GPS-Timepuls";
		gpios = <&gpio1 11 GPIO_ACTIVE_HIGH>;
		assert-falling-edge;
		status = "okay";
	};

	
	
	/*
	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_keys>;
		#address-cells = <1>;
		#size-cells = <0>;

		pps {
			compatible = "pps-gpio";
			label = "GPS-Timepuls";
			gpios = <&gpio1 11 GPIO_ACTIVE_HIGH>;
			assert-falling-edge;
			linux,code = <84>;
			status = "okay";
		};
	};

	gpio-reset {
		compatible = "linux,gpio-reset";

		wlan_reset:wlan {
			#reset-cells = <0>;
			gpios = <&gpio2 13 GPIO_ACTIVE_LOW>;
			asserted-state = <0>;
			duration-ms = <100>;
			auto;
		};
		gps_reset:gps {
			#reset-cells = <0>;
			gpios = <&gpio4 20 GPIO_ACTIVE_LOW>;
			asserted-state = <0>;
			duration-ms = <100>;
		};
	}

	ethernet_phy_reset:ethernet_phy {
		#reset-cells = <0>;
		gpios = <&gpio7 6 GPIO_ACTIVE_LOW>;
		asserted-state = <0>;
		duration-ms = <100>;
	};

*/
	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_usb_otg1_vbus: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			pinctrl-names = "default";
			regulator-name = "usb_otg1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};

		reg_usb_otg2_vbus: regulator@1 {
			compatible = "regulator-fixed";
			reg = <1>;
			pinctrl-names = "default";
			regulator-name = "usb_otg2_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};

		reg_psu_5v: regulator@2 {
			compatible = "regulator-fixed";
			reg = <2>;
			regulator-name = "PSU-5V0";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
		};

		reg_peri_3v3: regulator@3 {
			compatible = "regulator-fixed";
			reg = <3>;
			pinctrl-names = "default";
			regulator-name = "peri_3v3";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};

		reg_enet_3v3: regulator@4 {
			compatible = "regulator-fixed";
			reg = <4>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_enet_3v3>;
			regulator-name = "enet_3v3";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpios = <&gpio2 6 GPIO_ACTIVE_LOW>;
		};

		wlreg_on: fixedregulator@100 {
			compatible = "regulator-fixed";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-name = "wlreg_on";
			gpio = <&gpio2 13 0x04>;
			startup-delay-us = <0x11170>;
			regulator-always-on;
			enable-active-high;
		};
	};
};

